# SYNOPSIS

    #include "nanowww.h"
    nanowww::Client www;
    nanowww::Response;
    if (www.send_get(&res, "http://google.com")) {
        if (res.is_success()) {
            cout << res.content() << endl;
        }
    } else {
        cerr << res.errstr() << endl;
    }

